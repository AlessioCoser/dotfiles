// Place your key bindings in this file to overwrite the defaults
[
  { "key": "ctrl+alt+t", "command": "workbench.action.terminal.focus"},
  {
    "key": "ctrl+alt+t",
    "command": "workbench.action.closePanel",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+\\",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "shift+cmd+d",
    "command": "editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorHasSelection && !editorReadonly"
  },
  {
    "key": "cmd+d",
    "command": "-editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorHasSelection && !editorReadonly"
  },
  {
    "key": "alt+cmd+d",
    "command": "workbench.files.action.compareFileWith",
    "when": "activeEditor && !editorTextFocus"
  },
  {
    "key": "cmd+d",
    "command": "-workbench.files.action.compareFileWith",
    "when": "activeEditor && !editorTextFocus"
  },
  {
    "key": "shift+cmd+d",
    "command": "editor.action.duplicateSelection",
    "when": "editorHasSelection && editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+d",
    "command": "-editor.action.duplicateSelection",
    "when": "editorHasSelection && editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+d",
    "command": "-workbench.view.debug",
    "when": "viewContainer.workbench.view.debug.enabled"
  },
  {
    "key": "ctrl+cmd+down",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+down",
    "command": "-editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+cmd+up",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+up",
    "command": "-editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+r",
    "command": "testing.reRunLastRun"
  },
  {
    "key": "cmd+; l",
    "command": "-testing.reRunLastRun"
  },
  {
    "key": "ctrl+shift+c",
    "command": "testing.coverageLastRun"
  },
  {
    "key": "cmd+; shift+cmd+l",
    "command": "-testing.coverageLastRun"
  },
  {
    "key": "ctrl+alt+c",
    "command": "testing.coverageAll"
  },
  {
    "key": "cmd+; shift+cmd+a",
    "command": "-testing.coverageAll"
  },
  {
    "key": "ctrl+shift+c",
    "command": "testing.coverageAtCursor",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+; shift+cmd+c",
    "command": "-testing.coverageAtCursor",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+r",
    "command": "testing.runAtCursor",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+; c",
    "command": "-testing.runAtCursor",
    "when": "editorTextFocus"
  }
]
