{
    "workbench.colorCustomizations": {
      "[Material Theme]": {
        "sideBar.background": "#202c31",
        "editor.background": "#243238",
        "sideBar.border": "#243238",
        "terminal.border": "#243238",
        "panel.background": "#202c31"
      },
      "activityBarBadge.background": "#80CBC4",
      "activityBar.activeBorder": "#80CBC4",
      "list.activeSelectionForeground": "#80CBC4",
      "list.inactiveSelectionForeground": "#80CBC4",
      "list.highlightForeground": "#80CBC4",
      "scrollbarSlider.activeBackground": "#80CBC450",
      "editorSuggestWidget.highlightForeground": "#80CBC4",
      "textLink.foreground": "#80CBC4",
      "progressBar.background": "#80CBC4",
      "pickerGroup.foreground": "#80CBC4",
      "tab.activeBorder": "#80CBC4",
      "notificationLink.foreground": "#80CBC4",
      "editorWidget.resizeBorder": "#80CBC4",
      "editorWidget.border": "#80CBC4",
      "settings.modifiedItemIndicator": "#80CBC4",
      "settings.headerForeground": "#80CBC4",
      "panelTitle.activeBorder": "#80CBC4",
      "breadcrumb.activeSelectionForeground": "#80CBC4",
      "menu.selectionForeground": "#80CBC4",
      "menubar.selectionForeground": "#80CBC4",
      "editor.findMatchBorder": "#80CBC4",
      "selection.background": "#80CBC440"
    },
    "editor.fontSize": 17,
    "editor.scrollBeyondLastLine": false,
    "editor.minimap.enabled": false,
    "explorer.openEditors.visible": 0,
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "One Dark Pro",
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "onFocusChange",
    "files.exclude": {
      "**/__pycache__": true,
      "**/.classpath": true,
      "**/.DS_Store": true,
      "**/.factorypath": true,
      "**/.git": true,
      "**/.hg": true,
      "**/.project": true,
      "**/.serverless": true,
      "**/.settings": true,
      "**/.svn": true,
      "**/.vagrant": true,
      "**/CVS": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.vagrant/**": true,
        "**/.serverless/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/.vagrant": true,
        "**/.serverless": true
    },
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "editor.trimAutoWhitespace": true,
    "extensions.ignoreRecommendations": true,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "explorer.confirmDelete": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "workbench.activityBar.visible": true,
    "workbench.view.alwaysShowHeaderActions": true,
    "materialTheme.accent": "Teal",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.languageServer": "Default",
    "[html]": {
      "editor.defaultFormatter": "mohd-akram.vscode-html-format"
    },
    "markdown.marp.enableHtml": true,
    "markdown.marp.chromePath": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome",
    "js/ts.implicitProjectConfig.checkJs": true,
    "eslint.alwaysShowStatus": true,
    "window.menuBarVisibility": "toggle",
    "telemetry.telemetryLevel": "off",
    "[typescript]": {
      "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "[python]": {
      "editor.tabSize": 4
    },
    "editor.rulers": [
      120
    ],
    "workbench.editor.untitled.hint": "hidden",
    "debug.console.fontSize": 16,
    "terminal.integrated.fontSize": 16,
    "[dart]": {
      "editor.formatOnSave": true,
      "editor.formatOnType": true,
      "editor.rulers": [
        80
      ],
      "editor.selectionHighlight": false,
      "editor.suggest.snippetsPreventQuickSuggestions": false,
      "editor.suggestSelection": "first",
      "editor.tabCompletion": "onlySnippets",
      "editor.wordBasedSuggestions": false
    },
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.restoreWindows": "none",
    "window.zoomLevel": 1,
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
  }
